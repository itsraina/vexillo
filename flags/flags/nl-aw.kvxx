		<!-- The commented colors are from the British Flag Institute. -->
		<!-- I believe the NOC colors to be slightly more accurate. -->
		<color id="blue">
			<colorspec model="pantone" value="279"/>
			<colorspec model="cmyk" value="68 34 0 0"/>
			<colorspec model="rgb" value="80 145 205"/>
			<!-- colorspec model="pantone" value="300" -->
			<!-- colorspec model="cmyk" value="100 44 0 0" -->
			<!-- colorspec model="rgb" value="0 121 193" -->
		</color>
		<color id="yellow">
			<colorspec model="pantone" value="108"/>
			<colorspec model="cmyk" value="0 6 95 0"/>
			<colorspec model="rgb" value="255 229 18"/>
			<!-- colorspec model="pantone" value="109" -->
			<!-- colorspec model="cmyk" value="0 10 100 0" -->
			<!-- colorspec model="rgb" value="255 221 0" -->
		</color>
		<color id="white">
			<colorspec model="pantone" value="Safe"/>
			<colorspec model="cmyk" value="0 0 0 0"/>
			<colorspec model="rgb" value="255 255 255"/>
		</color>
		<color id="red">
			<colorspec model="pantone" value="186"/>
			<colorspec model="cmyk" value="0 100 81 4"/>
