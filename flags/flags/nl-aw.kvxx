		<symdef id="star4" d="
			M 0 -1 L 0.2357 -0.2357 L 1 0 L 0.2357 0.2357
			L 0 1 L -0.2357 0.2357 L -1 0 L -0.2357 -0.2357 Z
		"/>
	</defs>
	<hband x1="0" y1="0" x2="w" y2="h" bands="5" colors="blue yellow blue yellow blue" weights="12 1 1 1 3"/>
	<symbol symbol="star4" x="u*4" y="u*4" sx="u*3" sy="u*3" color="white"/>
	<symbol symbol="star4" x="u*4" y="u*4" sx="u*2.5" sy="u*2.5" color="red"/>
</flag>